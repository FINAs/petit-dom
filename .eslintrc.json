{
  "extends": "eslint:recommended",
  "ecmaFeatures": {
     "modules": true,
     "spread" : true,
     "restParams" : true
   },
   "env" : {
       "browser" : true,
       "node" : true,
       "es6" : true
   },
   "parserOptions": {
     "sourceType": "module",
     "ecmaFeatures": {
       "jsx": true
      }
   },
  "rules": {
    "no-unused-vars": ["error", { "argsIgnorePattern": "^_" }]
  }
}
